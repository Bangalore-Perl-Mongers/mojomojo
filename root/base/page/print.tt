[% SET page = pages.first %]
[% PROCESS header.tt %]
<h1 id="pageName">
  <small> [% title || config.application_name || "MojoMojo" %] </small>
  <br/>[%c.expand_wikiword(page.path)%] [%IF c.req.params.rev %]
  (Revision #[%c.req.params.rev%])[%END%]
</h1>
<div id="revision">
[% content.formatted(base) %]
</div>

<div class="byline">
  Revised on [% content.created.ymd %] at [% content.created.hms %]
    by [% content.creator.name %]
</div>

[% PROCESS footer.tt %]
