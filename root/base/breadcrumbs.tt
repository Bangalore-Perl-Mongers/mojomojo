<div id="breadcrumbs">
 [% FOREACH page IN path_pages %]
  [% IF loop.first %]
  <a href="[% base _ page.path %]">[% c.pref("name") || "MojoMojo" %]</a>
  [% ELSE %]
  / <form action="[% base _ page.path %]" method="POST">
     <a href="[% base _ page.path %]">[% page.name_orig %]</a>
     <input type="hidden" name="id">
    </form>
  [% END %]
 [% END %]
 [% FOREACH page IN proto_pages %]
 / <a class="protoPage" href="[% base _ page.path %]">[% page.name_orig %]</a>
 [% END %]
</div>
