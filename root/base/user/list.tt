[% WRAPPER page/wrapper.tt title = title || config.application_name || "MojoMojo" %]
[% PROCESS pager.tt %]
<ul>
[% WHILE (user=users.next) %]
<li>[%c.wikiword(user.link,base)%]<br/>
Last Changed 
[% FOREACH  changed IN user.pages %]
[% '&ndash;' UNLESS loop.first%] [%c.wikiword(changed.path,base)%]
[%END%]
</li>
[%END %]
</ul>
[% END %]

